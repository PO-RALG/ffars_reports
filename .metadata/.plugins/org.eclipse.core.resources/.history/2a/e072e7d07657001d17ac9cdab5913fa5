	<queryString language="SQL">
		<![CDATA[Select
    fwv.cname as council,
    fwv.name as facility,
    fwv.ftype as type,
    jv.date as apply_date,
    jv.created_at as entry_date,
    jv.number
from journal_vouchers jv
join facility_ward_view fwv on jv.facility_id = fwv.id
where jv.id = 12]]>
